(DEFINE (DOMAIN DOOR-EXAMPLE)
 (:PREDICATES (ARM-AUTOLOCK) (ARM-AUTOUNLOCK) (DOORS-CLOSED) (DOORS-LOCKED)
  (KEY-IGNITION) (REMOTE-UNLOCK-CMD) (ENGINE-ON) (TRANS-MODE-DRIVE)
  (HIGH-SPEED) (LOW-SPEED) (PREV-LOW-SPEED) (STATIONARY)
  (DISABLED-CONTROL-C4-AUTO-UNLOCK) (DISABLED-CONTROL-C3-ARM-AUTO-UNLOCK)
  (DISABLED-CONTROL-C2-AUTO-LOCK) (DISABLED-CONTROL-UNMARK-PREV-LOW-SPEED)
  (DISABLED-CONTROL-MARK-PREV-LOW-SPEED) (DISABLED-CONTROL-C1-ARM-AUTO-LOCK))
 (:FUNCTIONS (TOTAL-COST))
 (:ACTION E7-COMMAND-REMOTE-UNLOCK :PARAMETERS () :PRECONDITION
  (AND (DOORS-LOCKED)
       (NOT (REMOTE-UNLOCK-CMD))
       (DISABLED-CONTROL-C4-AUTO-UNLOCK)
       (DISABLED-CONTROL-C3-ARM-AUTO-UNLOCK)
       (DISABLED-CONTROL-C2-AUTO-LOCK)
       (DISABLED-CONTROL-UNMARK-PREV-LOW-SPEED)
       (DISABLED-CONTROL-MARK-PREV-LOW-SPEED)
       (DISABLED-CONTROL-C1-ARM-AUTO-LOCK))
  :EFFECT
  (AND (REMOTE-UNLOCK-CMD)
       (NOT (DISABLED-CONTROL-C3-ARM-AUTO-UNLOCK))
       (INCREASE (TOTAL-COST) 1)))
 (:ACTION E6-SPEED-LOW-TO-HIGH :PARAMETERS () :PRECONDITION
  (AND (ENGINE-ON)
       (TRANS-MODE-DRIVE)
       (LOW-SPEED)
       (DISABLED-CONTROL-C4-AUTO-UNLOCK)
       (DISABLED-CONTROL-C3-ARM-AUTO-UNLOCK)
       (DISABLED-CONTROL-C2-AUTO-LOCK)
       (DISABLED-CONTROL-UNMARK-PREV-LOW-SPEED)
       (DISABLED-CONTROL-MARK-PREV-LOW-SPEED)
       (DISABLED-CONTROL-C1-ARM-AUTO-LOCK))
  :EFFECT
  (AND (HIGH-SPEED)
       (NOT (LOW-SPEED))
       (NOT (DISABLED-CONTROL-C1-ARM-AUTO-LOCK))
       (INCREASE (TOTAL-COST) 1)))
 (:ACTION E6-SPEED-STAT-TO-LOW :PARAMETERS () :PRECONDITION
  (AND (ENGINE-ON)
       (TRANS-MODE-DRIVE)
       (STATIONARY)
       (DISABLED-CONTROL-C4-AUTO-UNLOCK)
       (DISABLED-CONTROL-C3-ARM-AUTO-UNLOCK)
       (DISABLED-CONTROL-C2-AUTO-LOCK)
       (DISABLED-CONTROL-UNMARK-PREV-LOW-SPEED)
       (DISABLED-CONTROL-MARK-PREV-LOW-SPEED)
       (DISABLED-CONTROL-C1-ARM-AUTO-LOCK))
  :EFFECT
  (AND (LOW-SPEED)
       (NOT (STATIONARY))
       (NOT (DISABLED-CONTROL-MARK-PREV-LOW-SPEED))
       (INCREASE (TOTAL-COST) 1)))
 (:ACTION E5-PUT-TRANSMISSION-DRIVE :PARAMETERS () :PRECONDITION
  (AND (ENGINE-ON)
       (DOORS-CLOSED)
       (NOT (TRANS-MODE-DRIVE))
       (DISABLED-CONTROL-C4-AUTO-UNLOCK)
       (DISABLED-CONTROL-C3-ARM-AUTO-UNLOCK)
       (DISABLED-CONTROL-C2-AUTO-LOCK)
       (DISABLED-CONTROL-UNMARK-PREV-LOW-SPEED)
       (DISABLED-CONTROL-MARK-PREV-LOW-SPEED)
       (DISABLED-CONTROL-C1-ARM-AUTO-LOCK))
  :EFFECT (AND (TRANS-MODE-DRIVE) (INCREASE (TOTAL-COST) 1)))
 (:ACTION E4-RUN-ENGINE :PARAMETERS () :PRECONDITION
  (AND (KEY-IGNITION)
       (NOT (ENGINE-ON))
       (DISABLED-CONTROL-C4-AUTO-UNLOCK)
       (DISABLED-CONTROL-C3-ARM-AUTO-UNLOCK)
       (DISABLED-CONTROL-C2-AUTO-LOCK)
       (DISABLED-CONTROL-UNMARK-PREV-LOW-SPEED)
       (DISABLED-CONTROL-MARK-PREV-LOW-SPEED)
       (DISABLED-CONTROL-C1-ARM-AUTO-LOCK))
  :EFFECT (AND (ENGINE-ON) (INCREASE (TOTAL-COST) 1)))
 (:ACTION E3-PUT-KEY-IN-IGNITION :PARAMETERS () :PRECONDITION
  (AND (NOT (KEY-IGNITION))
       (DISABLED-CONTROL-C4-AUTO-UNLOCK)
       (DISABLED-CONTROL-C3-ARM-AUTO-UNLOCK)
       (DISABLED-CONTROL-C2-AUTO-LOCK)
       (DISABLED-CONTROL-UNMARK-PREV-LOW-SPEED)
       (DISABLED-CONTROL-MARK-PREV-LOW-SPEED)
       (DISABLED-CONTROL-C1-ARM-AUTO-LOCK))
  :EFFECT (AND (KEY-IGNITION) (INCREASE (TOTAL-COST) 1)))
 (:ACTION E2-CLOSE-DOORS :PARAMETERS () :PRECONDITION
  (AND (NOT (DOORS-CLOSED))
       (DISABLED-CONTROL-C4-AUTO-UNLOCK)
       (DISABLED-CONTROL-C3-ARM-AUTO-UNLOCK)
       (DISABLED-CONTROL-C2-AUTO-LOCK)
       (DISABLED-CONTROL-UNMARK-PREV-LOW-SPEED)
       (DISABLED-CONTROL-MARK-PREV-LOW-SPEED)
       (DISABLED-CONTROL-C1-ARM-AUTO-LOCK))
  :EFFECT
  (AND (DOORS-CLOSED)
       (NOT (DISABLED-CONTROL-C2-AUTO-LOCK))
       (INCREASE (TOTAL-COST) 1)))
 (:ACTION E1-OPEN-DOORS :PARAMETERS () :PRECONDITION
  (AND (NOT (DOORS-LOCKED))
       (NOT (TRANS-MODE-DRIVE))
       (STATIONARY)
       (DOORS-CLOSED)
       (DISABLED-CONTROL-C4-AUTO-UNLOCK)
       (DISABLED-CONTROL-C3-ARM-AUTO-UNLOCK)
       (DISABLED-CONTROL-C2-AUTO-LOCK)
       (DISABLED-CONTROL-UNMARK-PREV-LOW-SPEED)
       (DISABLED-CONTROL-MARK-PREV-LOW-SPEED)
       (DISABLED-CONTROL-C1-ARM-AUTO-LOCK))
  :EFFECT (AND (NOT (DOORS-CLOSED)) (INCREASE (TOTAL-COST) 1)))
 (:ACTION CONTROL-C4-AUTO-UNLOCK :PARAMETERS () :PRECONDITION
  (AND (ARM-AUTOUNLOCK) (DOORS-LOCKED)) :EFFECT
  (AND (NOT (DOORS-LOCKED))
       (NOT (ARM-AUTOUNLOCK))
       (NOT (DISABLED-CONTROL-C2-AUTO-LOCK))))
 (:ACTION CONTROL-C3-ARM-AUTO-UNLOCK :PARAMETERS () :PRECONDITION
  (AND (REMOTE-UNLOCK-CMD)
       (DOORS-LOCKED)
       (NOT (HIGH-SPEED))
       (NOT (ARM-AUTOUNLOCK)))
  :EFFECT (AND (ARM-AUTOUNLOCK) (NOT (DISABLED-CONTROL-C4-AUTO-UNLOCK))))
 (:ACTION CONTROL-C2-AUTO-LOCK :PARAMETERS () :PRECONDITION
  (AND (ARM-AUTOLOCK) (DOORS-CLOSED) (NOT (DOORS-LOCKED))) :EFFECT
  (AND (DOORS-LOCKED)
       (NOT (ARM-AUTOLOCK))
       (NOT (DISABLED-CONTROL-C3-ARM-AUTO-UNLOCK))
       (NOT (DISABLED-CONTROL-C4-AUTO-UNLOCK))))
 (:ACTION CONTROL-UNMARK-PREV-LOW-SPEED :PARAMETERS () :PRECONDITION
  (AND (PREV-LOW-SPEED) (NOT (LOW-SPEED)) (NOT (HIGH-SPEED))) :EFFECT
  (AND (NOT (PREV-LOW-SPEED))))
 (:ACTION CONTROL-MARK-PREV-LOW-SPEED :PARAMETERS () :PRECONDITION
  (AND (LOW-SPEED) (NOT (PREV-LOW-SPEED))) :EFFECT (AND (PREV-LOW-SPEED)))
 (:ACTION CONTROL-C1-ARM-AUTO-LOCK :PARAMETERS () :PRECONDITION
  (AND (PREV-LOW-SPEED) (NOT (LOW-SPEED)) (HIGH-SPEED)) :EFFECT
  (AND (ARM-AUTOLOCK)
       (NOT (PREV-LOW-SPEED))
       (NOT (DISABLED-CONTROL-C2-AUTO-LOCK))))
 (:ACTION DISABLE-2-CONTROL-C4-AUTO-UNLOCK :PARAMETERS () :PRECONDITION
  (NOT (DOORS-LOCKED)) :EFFECT (DISABLED-CONTROL-C4-AUTO-UNLOCK))
 (:ACTION DISABLE-1-CONTROL-C4-AUTO-UNLOCK :PARAMETERS () :PRECONDITION
  (NOT (ARM-AUTOUNLOCK)) :EFFECT (DISABLED-CONTROL-C4-AUTO-UNLOCK))
 (:ACTION DISABLE-4-CONTROL-C3-ARM-AUTO-UNLOCK :PARAMETERS () :PRECONDITION
  (ARM-AUTOUNLOCK) :EFFECT (DISABLED-CONTROL-C3-ARM-AUTO-UNLOCK))
 (:ACTION DISABLE-3-CONTROL-C3-ARM-AUTO-UNLOCK :PARAMETERS () :PRECONDITION
  (HIGH-SPEED) :EFFECT (DISABLED-CONTROL-C3-ARM-AUTO-UNLOCK))
 (:ACTION DISABLE-2-CONTROL-C3-ARM-AUTO-UNLOCK :PARAMETERS () :PRECONDITION
  (NOT (DOORS-LOCKED)) :EFFECT (DISABLED-CONTROL-C3-ARM-AUTO-UNLOCK))
 (:ACTION DISABLE-1-CONTROL-C3-ARM-AUTO-UNLOCK :PARAMETERS () :PRECONDITION
  (NOT (REMOTE-UNLOCK-CMD)) :EFFECT (DISABLED-CONTROL-C3-ARM-AUTO-UNLOCK))
 (:ACTION DISABLE-3-CONTROL-C2-AUTO-LOCK :PARAMETERS () :PRECONDITION
  (DOORS-LOCKED) :EFFECT (DISABLED-CONTROL-C2-AUTO-LOCK))
 (:ACTION DISABLE-2-CONTROL-C2-AUTO-LOCK :PARAMETERS () :PRECONDITION
  (NOT (DOORS-CLOSED)) :EFFECT (DISABLED-CONTROL-C2-AUTO-LOCK))
 (:ACTION DISABLE-1-CONTROL-C2-AUTO-LOCK :PARAMETERS () :PRECONDITION
  (NOT (ARM-AUTOLOCK)) :EFFECT (DISABLED-CONTROL-C2-AUTO-LOCK))
 (:ACTION DISABLE-3-CONTROL-UNMARK-PREV-LOW-SPEED :PARAMETERS () :PRECONDITION
  (HIGH-SPEED) :EFFECT (DISABLED-CONTROL-UNMARK-PREV-LOW-SPEED))
 (:ACTION DISABLE-2-CONTROL-UNMARK-PREV-LOW-SPEED :PARAMETERS () :PRECONDITION
  (LOW-SPEED) :EFFECT (DISABLED-CONTROL-UNMARK-PREV-LOW-SPEED))
 (:ACTION DISABLE-1-CONTROL-UNMARK-PREV-LOW-SPEED :PARAMETERS () :PRECONDITION
  (NOT (PREV-LOW-SPEED)) :EFFECT (DISABLED-CONTROL-UNMARK-PREV-LOW-SPEED))
 (:ACTION DISABLE-2-CONTROL-MARK-PREV-LOW-SPEED :PARAMETERS () :PRECONDITION
  (PREV-LOW-SPEED) :EFFECT (DISABLED-CONTROL-MARK-PREV-LOW-SPEED))
 (:ACTION DISABLE-1-CONTROL-MARK-PREV-LOW-SPEED :PARAMETERS () :PRECONDITION
  (NOT (LOW-SPEED)) :EFFECT (DISABLED-CONTROL-MARK-PREV-LOW-SPEED))
 (:ACTION DISABLE-3-CONTROL-C1-ARM-AUTO-LOCK :PARAMETERS () :PRECONDITION
  (LOW-SPEED) :EFFECT (DISABLED-CONTROL-C1-ARM-AUTO-LOCK))
 (:ACTION DISABLE-2-CONTROL-C1-ARM-AUTO-LOCK :PARAMETERS () :PRECONDITION
  (NOT (HIGH-SPEED)) :EFFECT (DISABLED-CONTROL-C1-ARM-AUTO-LOCK))
 (:ACTION DISABLE-1-CONTROL-C1-ARM-AUTO-LOCK :PARAMETERS () :PRECONDITION
  (NOT (PREV-LOW-SPEED)) :EFFECT (DISABLED-CONTROL-C1-ARM-AUTO-LOCK)))
